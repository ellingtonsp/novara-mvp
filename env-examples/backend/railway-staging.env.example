# Railway Staging Environment Variables
# Copy these values to your Railway staging project's Variables tab

# Required for Railway staging deployment
NODE_ENV=staging
USE_LOCAL_DATABASE=false
DATABASE_TYPE=airtable

# Airtable Configuration (Required for staging)
AIRTABLE_API_KEY=your_staging_airtable_api_key_here
AIRTABLE_BASE_ID=your_staging_airtable_base_id_here

# JWT Configuration (Required for staging)
JWT_SECRET=your_staging_jwt_secret_min_64_characters_long_here

# CORS Configuration (Staging frontend)
CORS_ORIGIN=https://novara-mvp-staging.vercel.app

# Logging (More verbose for staging)
LOG_LEVEL=debug

# Note: PORT is automatically provided by Railway as $PORT
# Do NOT set PORT manually in Railway variables

# Staging-specific settings
ENABLE_DEBUG_LOGGING=true
ENABLE_REQUEST_LOGGING=true 