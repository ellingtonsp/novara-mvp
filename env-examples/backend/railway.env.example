# Railway Environment Variables
# Copy these values to your Railway project's Variables tab

# Required for Railway deployment
NODE_ENV=production
USE_LOCAL_DATABASE=false
DATABASE_TYPE=airtable

# Airtable Configuration (Required)
AIRTABLE_API_KEY=your_production_airtable_api_key_here
AIRTABLE_BASE_ID=your_production_airtable_base_id_here

# JWT Configuration (Required)
JWT_SECRET=your_production_jwt_secret_min_64_characters_long_here

# CORS Configuration
CORS_ORIGIN=https://novara-mvp.vercel.app

# Logging
LOG_LEVEL=info

# Note: PORT is automatically provided by Railway as $PORT
# Do NOT set PORT manually in Railway variables 