# Production Environment Variables
NODE_ENV=production
PORT=8080
DATABASE_TYPE=airtable
USE_LOCAL_DATABASE=false

# Airtable Configuration
AIRTABLE_API_KEY=your_production_airtable_api_key_here
AIRTABLE_BASE_ID=app5QWCcVbCnVg2Gg

# JWT Configuration
JWT_SECRET=production_super_secret_jwt_key_different_from_staging
JWT_EXPIRES_IN=90d

# Security Configuration
CORS_ORIGIN=https://novara-mvp.vercel.app
TRUST_PROXY=true
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Performance Configuration
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
CACHE_TTL=3600

# Monitoring Configuration
SENTRY_DSN=your_production_sentry_dsn_here
SENTRY_ENVIRONMENT=production
ENABLE_PERFORMANCE_MONITORING=true

# PostHog Configuration (Backend Analytics)
POSTHOG_API_KEY=phc_5imIla9RYl1YPLFcrXNOlbbZfB6tAY1EKFNq7WFsCbt
POSTHOG_HOST=https://app.posthog.com

# Feature Flags
ENABLE_CM01_FEATURES=true
ENABLE_AN01_FEATURES=true
ENABLE_SENTIMENT_ANALYSIS=true
ENABLE_MEDICATION_STATUS=true
ENABLE_DYNAMIC_QUESTIONS=true

# Logging Configuration
LOG_LEVEL=warn
ENABLE_STRUCTURED_LOGGING=true
ENABLE_REQUEST_LOGGING=true

# Health Check Configuration
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=30000 