# Novara MVP - Frontend Production Environment
# Copy this file to .env.production and customize as needed

# API Configuration
VITE_API_URL=https://novara-mvp-production.up.railway.app

# Environment
VITE_ENV=production
VITE_NODE_ENV=production

# Development Settings
VITE_DEBUG=false
VITE_LOG_LEVEL=warn
VITE_ENABLE_HOT_RELOAD=false

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Analytics Configuration
VITE_POSTHOG_API_KEY=phc_5imIla9RYl1YPLFcrXNOlbbZfB6tAY1EKFNq7WFsCbt
VITE_POSTHOG_HOST=https://app.posthog.com

# Error Tracking
VITE_SENTRY_DSN=your_production_sentry_dsn_here
VITE_SENTRY_ENVIRONMENT=production

# Performance Monitoring
VITE_ENABLE_WEB_VITALS=true
VITE_ENABLE_RESOURCE_MONITORING=true

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_PWA_NAME=Novara MVP
VITE_PWA_SHORT_NAME=Novara
VITE_PWA_DESCRIPTION=Fertility journey support and insights
VITE_PWA_THEME_COLOR=#6366f1
VITE_PWA_BACKGROUND_COLOR=#ffffff

# Feature Toggles
VITE_ENABLE_CM01_FEATURES=true
VITE_ENABLE_AN01_FEATURES=true
VITE_ENABLE_MEDICATION_STATUS=true
VITE_ENABLE_SENTIMENT_ANALYSIS=true
VITE_ENABLE_DYNAMIC_QUESTIONS=true
VITE_ENABLE_PERSONALIZED_INSIGHTS=true 