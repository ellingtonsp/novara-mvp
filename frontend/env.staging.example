# Novara MVP - Frontend Staging Environment
# Copy this file to .env.staging and customize as needed

# API Configuration
VITE_API_URL=https://novara-staging.up.railway.app

# Environment
VITE_ENV=staging
VITE_NODE_ENV=staging

# Development Settings
VITE_DEBUG=false
VITE_LOG_LEVEL=info
VITE_ENABLE_HOT_RELOAD=false

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_DEBUG_PANEL=false

# Google Analytics (staging)
VITE_GA_MEASUREMENT_ID=G-QP9XJD6QFS

# Staging URLs
VITE_BACKEND_URL=https://novara-staging.up.railway.app
VITE_FRONTEND_URL=https://novara-mvp-staging.vercel.app

# Staging Features
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_OFFLINE_MODE=false 