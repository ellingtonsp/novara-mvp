# Novara MVP - Frontend Development Environment
# Copy this file to .env.development and customize as needed

# API Configuration
VITE_API_URL=http://localhost:9002

# Environment
VITE_ENV=development
VITE_NODE_ENV=development

# Development Settings
VITE_DEBUG=true
VITE_LOG_LEVEL=debug
VITE_ENABLE_HOT_RELOAD=true

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_DEBUG_PANEL=true

# Google Analytics (disabled in development)
VITE_GA_MEASUREMENT_ID=

# Development URLs
VITE_BACKEND_URL=http://localhost:9002
VITE_FRONTEND_URL=http://localhost:4200

# Development Features
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_OFFLINE_MODE=false 