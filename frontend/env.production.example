# Novara MVP - Frontend Production Environment
# Copy this file to .env.production and customize as needed

# API Configuration
VITE_API_URL=https://novara-mvp-production.up.railway.app

# Environment
VITE_ENV=production
VITE_NODE_ENV=production

# Development Settings
VITE_DEBUG=false
VITE_LOG_LEVEL=warn
VITE_ENABLE_HOT_RELOAD=false

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_DEBUG_PANEL=false

# Google Analytics (production)
VITE_GA_MEASUREMENT_ID=G-QP9XJD6QFS

# Error Tracking (Sentry)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Production URLs
VITE_BACKEND_URL=https://novara-mvp-production.up.railway.app
VITE_FRONTEND_URL=https://novara-mvp.vercel.app

# Production Features
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_OFFLINE_MODE=false

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true 